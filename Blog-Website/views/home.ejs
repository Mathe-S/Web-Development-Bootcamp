
<%- include("partials/header") -%>


<h1>Home</h1>
<p><%=homeStartingContent %></p>
<% posts.forEach(element => { %>
 <h1>   <%= element.title %></h1>
 <% if(element.post.length>100){ %>
 <p> <%= element.post.substring(0,100) %>.. <a href="/posts/<%=element.title %>">Read More</a></p>
 <% } else { %>
    <p> <%= element.post %> <a href="/posts/<%=element._id %>">Read More</a></p>
<% }}); %>


<%- include("partials/footer") -%>